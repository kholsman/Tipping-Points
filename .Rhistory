# get difference in signs
getdelta<-function(xx,rnd = rndN2){
nn        <- length(xx)
xx        <- round(xx,rndN2)
delta     <- rep(NA,nn)
updn      <- c(0, diff(sign(xx)))
#updn[xx==0]<-0
ix        <- which(updn != 0)
#(xx[ix] + xx[ix-1])/2
sign(updn)[ix]
delta[ix] <- 1
return(list(delta=delta,ix=ix,updn=updn,xx=xx))
}
